CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PUBLIC.PRODUCTS (PRODID VARCHAR(8) NOT NULL PRIMARY KEY,PRODNAME VARCHAR(50),PRODDES VARCHAR(2000),PRODIMAGE VARCHAR(30),PRODPRICE DECIMAL(10,2),PRODKEYWORDS VARCHAR NULL)
CREATE MEMORY TABLE PUBLIC.USERS (USERID VARCHAR(8) NOT NULL PRIMARY KEY,USERNAME VARCHAR(50),USEREMAIL VARCHAR(50),USERPASSWORD VARCHAR(20))
CREATE MEMORY TABLE PUBLIC.CARTITEM (USERID VARCHAR(8), PRODID VARCHAR(8), QUANTITY INTEGER, FOREIGN KEY (USERID) REFERENCES PUBLIC.USERS(USERID), FOREIGN KEY (PRODID) REFERENCES PUBLIC.PRODUCTS(PRODID), PRIMARY KEY (USERID, PRODID))
CREATE MEMORY TABLE PUBLIC.ORDERHISTORY (USERID VARCHAR(8), PRODID VARCHAR(8), ORDERDATE DATE, FOREIGN KEY (USERID) REFERENCES PUBLIC.USERS(USERID), FOREIGN KEY (PRODID) REFERENCES PUBLIC.PRODUCTS(PRODID), PRIMARY KEY (USERID, PRODID))
